-- database: ./fabio.db
SELECT Prestiti.utente, Prestiti.data_prestito, Libri.titolo, Libri.autore_id
FROM Prestiti
JOIN Libri ON Prestito.libro_id = Libri.id
WHERE Prestiti.utente = 'Alessandro Verdi';

SELECT Prestiti.utente, Prestiti.Data_prestito, Libri.titolo, Libri.autore_id
FROM Prestiti
JOIN Libri ON Prestiti.libro_id = Libri.id
WHERE Prestiti.utente = 'Alessandro Verdi';
SELECT * FROM Prestiti;
